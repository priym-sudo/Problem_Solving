int ic=0,jc=0;
        for(int i=0;i<apples.length;i++)
        {
            if(a+apples[i]>=s && a+apples[i]<=t)
            {
                ic++;
            }
        }
        System.out.println(ic);
        for (int j=0;j<oranges.length;j++)
        {
            if(b+oranges[j]>=s && b+oranges[j]<=t)
            {
                jc++;
            }
        }
        System.out.println(jc);
