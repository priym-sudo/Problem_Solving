 int vcount=0,alt=0;
        for (int i=0;i<n;i++)
        {
            char ch=s.charAt(i);
            if(ch=='U')
            {
                alt++;
                if(alt==0)
                {
                    vcount++;
                }
            }
            else
            {
                alt--;
            }
        }
        return vcount;
