int sum1=0,sum2=0,diff=0;
        for(int i=0;i<arr.size();i++)
        {
            for (int j=0;j<arr.size();j++)
            {
             if(i==j)
             {
                 sum1=sum1+arr.get(i).get(j);
             }
             if(i+j==arr.size()-1)
             {
                 sum2=sum2+arr.get(i).get(j);
             }
            }
        }
        diff=Math.abs(sum1-sum2);
        return diff;
