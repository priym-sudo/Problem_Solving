int count=0,sum=0;
        for (int i=0;i<n;i++)
        {
            for (int j=i+1;j<n;j++)
            { 
                if(i<j)
                {                  
                  if((ar[i]+ar[j])%k==0)
                  {
                      count++;
                  }
                 }
                    
                }
                    }
                    return count;
